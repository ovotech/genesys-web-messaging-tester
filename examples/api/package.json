{
  "name": "@examples/api",
  "description": "Example of interacting with Web Messenger deployment using Jest, TS and JS",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "lint": "eslint -c ../../.eslintrc.json 'src/**/*.ts' '__tests__/**/*.ts'",
    "test": "jest __tests__",
    "test:js": "node ../api/src/js-script.js",
