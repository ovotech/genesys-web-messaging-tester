  },
  "devDependencies": {
    "@ovotech/genesys-web-messaging-tester": "2.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^14.14.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "dotenv": "^16.3.1",
    "jest": "^29.0.3",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.8.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "../../jest.json"
  }
}
