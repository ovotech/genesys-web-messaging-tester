{
  "name": "@ovotech/genesys-web-messaging-tester",
  "version": "1.0.5",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "Apache-2.0",
  "repository": "git@github.com:ovotech/genesys-web-messaging-tester.git",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "genesys",
    "web-messaging",
    "messenger",
    "test"
  ],
  "author": {
    "name": "Lucas Woodward"
  },
  "scripts": {
    "build": "rm -rf lib/ && tsc -p tsconfig.json",
    "build:readme": "build-readme README.md",
