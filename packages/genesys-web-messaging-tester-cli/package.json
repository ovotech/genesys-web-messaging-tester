    "@types/node": "^14.14.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^29.0.3",
    "jest-when": "^3.5.0",
    "prettier": "^2.3.2",
    "strip-ansi": "^6.0.1",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
